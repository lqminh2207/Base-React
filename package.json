{
  "name": "shopping-web",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "start:dev": "vite --mode development --port 3600",
    "dev": "concurrently \"yarn start:dev --host\"",
    "prod": "vite --mode production --port 3600 --host",
    "dev:https": "vite --mode development --port 3600 --host --https",
    "dev:local": "vite --mode dev --port 3600 --host",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,less,sass,scss,md}\"",
    "lint": "yarn format && eslint --cache --max-warnings=0 --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts-check": "yarn tsc --noEmit",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens ./src/themes/index.ts",
    "postinstall": "yarn run gen:theme-typings",
    "test": "vitest",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.1.0",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@react-oauth/google": "^0.12.1",
    "@tanstack/react-query": "^5.51.16",
    "@tanstack/react-query-devtools": "^5.51.16",
    "@types/lodash-es": "^4.17.12",
    "axios": "^1.7.2",
    "chakra-dayzed-datepicker": "^0.2.8",
    "chakra-react-select": "^4.6.0",
    "dayjs": "^1.11.7",
    "dayzed": "^3.2.3",
    "dompurify": "^3.0.3",
    "firebase": "^9.22.1",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "html-react-parser": "^4.0.0",
    "i18next": "^23.2.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "libphonenumber-js": "^1.10.30",
    "lodash-es": "^4.17.21",
    "logt": "^1.5.0",
    "quill-blot-formatter": "^1.0.5",
    "quill-image-drop-module": "^1.0.3",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.9",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^13.0.1",
    "react-icons": "^4.8.0",
    "react-phone-input-2": "^2.15.1",
    "react-player": "^2.12.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.11.2",
    "react-social-media-embed": "^2.3.5",
    "react-textarea-autosize": "^8.4.1",
    "terser": "^5.31.3",
    "web-vitals": "^3.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-legacy": "^4.0.4",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "5.1.6",
    "vite": "^5.3.5",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-remove-console": "^2.1.1",
    "vitest": "^0.32.2"
  }
}
